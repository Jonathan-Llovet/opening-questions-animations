{
    "header": {
        "grc": "κοιναὶ ἔννοιαι",
        "lat": "Communes Animi Conceptiones",
        "eng": "Common Notions"
    },
    "common_notions": [
        {
            "number": {
                "ordinal": {
                    "grc": "πρῶτα",
